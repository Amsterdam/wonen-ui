{
  "name": "@amsterdam/wonen-ui",
  "version": "1.0.35",
  "private": false,
  "description": "Shared functionality to be used by apps for 'Wonen'",
  "main": "index.js",
  "scripts": {
    "start": "npm run storybook",
    "test": "SKIP_PREFLIGHT_CHECK=true react-scripts test --watchAll=false",
    "test:coverage": "SKIP_PREFLIGHT_CHECK=true react-scripts test --coverage --watchAll=false",
    "lint": "eslint --max-warnings 0 --cache --ext .ts,.tsx src",
    "storybook:build": "build-storybook -o docs",
    "storybook": "start-storybook -p 9001 -c .storybook",
    "build": "rm -Rf ./dist && tsc ./src/index.ts --strict --esModuleInterop --declaration --skipLibCheck --jsx react --outDir dist && cp ./package.json ./dist/package.json"
  },
  "repository": {
    "type": "git",
    "url": ""
  },
  "keywords": [],
  "author": "",
  "license": "MIT",
  "peerDependencies": {
    "@amsterdam/asc-assets": ">= 0.29",
    "@amsterdam/asc-ui": ">= 0.29",
    "@material-ui/core": "^4.11.3",
    "@material-ui/icons": "^4.11.2",
    "react": ">= 16",
    "react-dom": ">= 16",
    "styled-components": ">= 5"
  },
  "dependencies": {
    "dompurify": "^2.3.8",
    "lodash.get": "^4.4.2",
    "lodash.pickby": "^4.6.0",
    "moment": "^2.29.3",
    "resize-observer-polyfill": "^1.5.1"
  },
  "devDependencies": {
    "@amsterdam/asc-assets": "^0.36.0",
    "@amsterdam/asc-ui": "^0.36.0",
    "@material-ui/core": "^4.12.4",
    "@material-ui/icons": "^4.11.3",
    "@storybook/addon-essentials": "^6.5.9",
    "@storybook/react": "^6.5.9",
    "@types/dompurify": "^2.2.3",
    "@types/enzyme": "^3.10.12",
    "@types/enzyme-adapter-react-16": "^1.0.6",
    "@types/jest": "^29.0.3",
    "@types/lodash.get": "^4.4.7",
    "@types/lodash.pickby": "^4.6.7",
    "@types/react": "^17.0.0",
    "@types/react-dom": "^18.0.3",
    "@types/styled-components": "^5.1.25",
    "@typescript-eslint/eslint-plugin": "^4.19.0",
    "@typescript-eslint/parser": "^4.19.0",
    "@wojtekmaj/enzyme-adapter-react-17": "^0.6.7",
    "enzyme": "^3.11.0",
    "eslint": "^7.22.0",
    "eslint-plugin-storybook": "^0.5.12",
    "eslint-plugin-unused-imports": "^1.1.0",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-scripts": "^4.0.3",
    "styled-components": "^5.3.5",
    "typescript": "^4.7.4"
  },
  "jest": {
    "collectCoverageFrom": [
      "src/**/*.{ts,tsx}",
      "!src/**/*.stories.{ts,tsx}",
      "!src/**/*.d.ts",
      "!src/**/index.{ts,tsx}"
    ]
  },
  "publishConfig": {
    "access": "public"
  }
}
