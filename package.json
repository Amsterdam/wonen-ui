{
  "name": "@amsterdam/wonen-ui",
  "version": "0.0.7",
  "private": false,
  "description": "Shared functionality to be used by apps for 'Wonen'",
  "main": "index.js",
  "scripts": {
    "test": "react-scripts test --watchAll=false",
    "test:coverage": "react-scripts test --coverage --watchAll=false",
    "lint": "eslint --max-warnings 0 --cache --ext .ts,.tsx src",
    "storybook": "start-storybook -p 9001 -c .storybook",
    "build": "rm -Rf ./dist && tsc ./src/index.ts --strict --esModuleInterop --declaration --skipLibCheck --jsx react --outDir dist && cp ./package.json ./dist/package.json",
    "publish": "npm version patch && cp package.json dist/ && cd dist && npm publish"
  },
  "repository": {
    "type": "git",
    "url": ""
  },
  "keywords": [],
  "author": "",
  "license": "MIT",
  "peerDependencies": {
    "react": "^16.14.0",
    "react-dom": "^16.14.0"
  },
  "devDependencies": {
    "@storybook/react": "^6.1.21",
    "@types/enzyme": "3.10.8",
    "@types/enzyme-adapter-react-16": "1.0.6",
    "@typescript-eslint/eslint-plugin": "^4.19.0",
    "@typescript-eslint/parser": "^4.19.0",
    "enzyme": "3.11.0",
    "enzyme-adapter-react-16": "^1.15.2",
    "eslint": "^7.22.0",
    "eslint-plugin-unused-imports": "^1.1.0",
    "ts-jest": "26.5.4",
    "typescript": "^4.2.3",
    "react": "^16.14.0",
    "react-dom": "^16.14.0",
    "react-scripts": "^4.0.3"
  },
  "jest": {
    "collectCoverageFrom": [
      "src/**/*.{ts,tsx}",
      "!src/**/*.stories.{ts,tsx}",
      "!src/**/*.d.ts",
      "!src/**/index.{ts,tsx}"
    ]
  },
  "publishConfig": {
    "access": "public"
  }
}
